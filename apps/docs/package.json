{
  "name": "@pierre/js-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "bun run build:deps && bun run build:next",
    "build:next": "next build --turbopack",
    "build:deps": "bun run build:deps:diffs && bun run build:deps:tree && bun run build:deps:elements-next && bun run build:deps:registry",
    "build:deps:diffs": "output=$(cd ../../packages/diffs && bun run build 2>&1) && echo '[diffs] Successfully cleaned and built.' || (echo \"$output\" >&2 && exit 1)",
    "build:deps:tree": "output=$(cd ../../packages/file-tree && bun run build 2>&1) && echo '[tree] Successfully cleaned and built.' || (echo \"$output\" >&2 && exit 1)",
    "build:deps:elements-next": "(cd ../../packages/storage-elements-next && bun run build)",
    "build:deps:registry": "shadcn build",
    "dev": "bun run build:deps && concurrently \"bun run dev:deps:diffs\" \"bun run dev:deps:tree\" \"bun run dev:next\" --names \"diffs,tree,docs\" --prefix-colors \"blue,green,yellow\"",
    "dev:next": "next dev --turbopack",
    "dev:deps:diffs": "(cd ../../packages/diffs && bun run dev)",
    "dev:deps:tree": "(cd ../../packages/file-tree && bun run dev)",
    "prod": "bun run build && next start",
    "start": "next start",
    "tsc": "bun run build:deps && tsc --noEmit --pretty"
  },
  "dependencies": {
    "@icons-pack/react-simple-icons": "catalog:",
    "@pierre/diffs": "workspace:*",
    "@pierre/file-tree": "workspace:*",
    "@pierre/storage-elements-next": "workspace:*",
    "@radix-ui/react-avatar": "catalog:",
    "@radix-ui/react-dialog": "catalog:",
    "@radix-ui/react-dropdown-menu": "catalog:",
    "@radix-ui/react-label": "catalog:",
    "@radix-ui/react-navigation-menu": "catalog:",
    "@radix-ui/react-popover": "catalog:",
    "@radix-ui/react-select": "catalog:",
    "@radix-ui/react-separator": "catalog:",
    "@radix-ui/react-slot": "catalog:",
    "@radix-ui/react-switch": "catalog:",
    "@radix-ui/react-toggle": "catalog:",
    "@radix-ui/react-toggle-group": "catalog:",
    "@radix-ui/react-tooltip": "catalog:",
    "@radix-ui/react-use-controllable-state": "catalog:",
    "@shikijs/transformers": "catalog:",
    "babel-plugin-react-compiler": "catalog:",
    "class-variance-authority": "catalog:",
    "clsx": "catalog:",
    "cmdk": "catalog:",
    "hast-util-heading-rank": "catalog:",
    "hast-util-to-string": "catalog:",
    "lucide-react": "catalog:",
    "next": "catalog:",
    "next-mdx-remote": "catalog:",
    "next-themes": "catalog:",
    "postcss": "catalog:",
    "postcss-import": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "remark-gfm": "catalog:",
    "shadcn": "catalog:",
    "shiki": "catalog:",
    "sonner": "catalog:",
    "tailwind-merge": "catalog:",
    "tailwindcss": "catalog:",
    "tw-animate-css": "catalog:",
    "unist-util-visit": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "catalog:",
    "@types/hast": "catalog:",
    "@types/mdast": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "concurrently": "catalog:",
    "typescript": "catalog:"
  },
  "overrides": {
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3"
  }
}
