{
  "name": "@pierre/solid-diff-demo",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vinxi dev",
    "build": "bun run build:deps && vinxi build",
    "build:deps": "bun run build:deps:diffs",
    "build:deps:diffs": "output=$(cd ../../packages/diffs && bun run build 2>&1) && echo '[diffs] Successfully cleaned and built.' || (echo \"$output\" >&2 && exit 1)",
    "start": "vinxi start",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@pierre/diffs": "workspace:",
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.15.3",
    "@solidjs/start": "^1.0.14",
    "solid-js": "^1.9.10",
    "vinxi": "^0.5.8"
  },
  "devDependencies": {
    "@types/bun": "1.3.2",
    "typescript": "catalog:",
    "vite": "^6.0.5"
  }
}
