{
  "name": "@pierre/js-demo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "bun run build:diffs && vite build",
    "build:diffs": "(cd ../../packages/precision-diffs && bun run build)",
    "build-types": "tsc --build",
    "dev": "bun run build:diffs && concurrently \"bun run dev:diffs\" \"bun run dev:vite\" --names \"diffs,vite\" --prefix-colors \"blue,green\"",
    "dev:diffs": "(cd ../../packages/precision-diffs && bun run dev)",
    "dev:vite": "vite",
    "preview": "bun run preview",
    "prod": "bun run build && bun run preview",
    "tsc": "tsc --noEmit --pretty"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitejs/plugin-react": "catalog:",
    "concurrently": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:"
  },
  "dependencies": {
    "@pierre/precision-diffs": "workspace:*",
    "react": "catalog:",
    "react-dom": "catalog:",
    "shiki": "catalog:"
  }
}
